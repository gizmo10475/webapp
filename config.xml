<?xml version='1.0' encoding='utf-8'?>
<widget id="dbwebb.projekt" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Projekt</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="hdpi" src="res/screen/android/mipmap-hdpi/ic_launcher.png" />
        <icon density="mdpi" src="res/screen/android/mipmap-mdpi/ic_launcher.png" />
        <icon density="xhdpi" src="res/screen/android/mipmap-xhdpi/ic_launcher.png" />
        <icon density="xxhdpi" src="res/screen/android/mipmap-xxhdpi/ic_launcher.png" />
        <icon density="xxxhdpi" src="res/screen/android/mipmap-xxxhdpi/ic_launcher.png" />

        <splash density="port-ldpi" src="res/screen/android/mipmap-mdpi/ic_launcher.png" />
        <splash density="port-mdpi" src="res/screen/android/mipmap-mdpi/ic_launcher.png" />
        <splash density="port-hdpi" src="res/screen/android/mipmap-hdpi/ic_launcher.png" />
        <splash density="port-xhdpi" src="res/screen/android/mipmap-xhdpi/ic_launcher.png" />
        <splash density="port-xxhdpi" src="res/screen/android/mipmap-xxhdpi/ic_launcher.png" />
        <splash density="port-xxxhdpi" src="res/screen/android/mipmap-xxxhdpi/ic_launcher.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
